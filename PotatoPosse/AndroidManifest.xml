            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>>
        </activity>
        <activity android:name=".activities.SendEmailActivity" >
        </activity>
        <activity android:name=".activities.TakePictureActivity" >
        </activity>
        <activity android:name=".activities.SymptomListActivity" >
        </activity>
        <activity android:name=".activities.SummaryActivity" >
        </activity>
        <activity android:name=".activities.TestActivity">
